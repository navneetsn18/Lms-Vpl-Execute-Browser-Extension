{
    "manifest_version": 2,
    "name": "Run LMS",
    "version": "1.1",
    "description": "VPL auto execute extension",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "content_scripts": [{
        "matches": ["http://lms.galgotiasuniversity.edu.in/mod/vpl/*"],
        "js": ["content.js"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon16.png"
    }
}